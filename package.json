{
  "name": "playing-content-services",
  "version": "1.0.0",
  "description": "Content management microservices for playful application",
  "author": "Forafish",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "publish": "git push origin --tags && git push origin",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "compile": "rimraf -rf lib/ && babel src/ -d lib/ --copy-files",
    "watch": "babel --watch src/ --source-maps inline -d lib/ --copy-files",
    "lint": "eslint-if-supported semistandard --fix",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "test": "npm run compile && npm run lint && npm run coverage"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "concat-stream": "^1.6.0",
    "date-fns": "^1.28.5",
    "dauria": "^2.0.0",
    "feathers-authentication": "^1.2.3",
    "feathers-authentication-hooks": "^0.1.4",
    "feathers-authentication-jwt": "^0.3.1",
    "feathers-authentication-local": "^0.3.4",
    "feathers-hooks": "^2.0.0",
    "feathers-hooks-common": "^3.5.5",
    "feathers-memory": "^1.1.0",
    "from2": "^2.3.0",
    "glob": "^7.1.1",
    "limax": "^1.5.0",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.17",
    "mongoose": "^4.11.1",
    "mongoose-random": "^0.1.1",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-unique-array": "^0.2.0",
    "nats": "^0.7.20",
    "pluralize": "^5.0.0",
    "shortid": "^2.2.8",
    "uberproto": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-env": "^1.4.0",
    "eslint-if-supported": "^1.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^3.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "rimraf": "^2.6.1"
  }
}
